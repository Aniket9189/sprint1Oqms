package com.abc.oqms.repository;

import java.util.List;
import java.util.Optional;

import org.springframework.data.jpa.repository.JpaRepository;

import com.abc.oqms.entity.Quiz;
import com.abc.oqms.entity.Round;

public interface QuizRepository extends JpaRepository<Quiz , Integer>
{
	public Optional<Quiz> findById(long quizId);

	public void save(Round round);

	public List<Round> findAll1();

	public List<Round> findById1(long roundId);

	public List<Quiz> findById2(long quizId);

}
