package com.abc.oqms.service;

import java.util.List;

import com.abc.oqms.entity.Quiz;
import com.abc.oqms.entity.Round;
import com.abc.oqms.exception.DuplicateEntryException;
import com.abc.oqms.exception.QuizNotFoundException;
import com.abc.oqms.exception.RoundNotFoundException;
public interface QuizService 
{
	public void addQuiz(Quiz quiz);

	void delete(long quizId) ;

	void updateQuiz(Quiz quiz);

	List<Quiz> searchByQuizId(long quizId);

	List<Quiz> searchAll();

	void delete1(long roundId);

	void addRound1(Round round) throws DuplicateEntryException;

	void updateRound1(Round round) throws RoundNotFoundException;

	List<Round> searchAll1();

	List<Round> searchByRoundId(long roundId);
	

}
